{
  "name": "Search Parameters",
  "description": "A extension to modify the search parameters easier!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/sp16.png",
      "32": "/images/sp32.png",
      "48": "/images/sp48.png",
      "128": "/images/sp128.png"
    }
  },
  "icons": {
    "16": "/images/sp16.png",
    "32": "/images/sp32.png",
    "48": "/images/sp48.png",
    "128": "/images/sp128.png"
  }
}
